{
    "name": "react-redux-mui-boilerplate",
    "version": "1.0.0",
    "description": "Boilerplate project for React, React-Redux, and Material UI.",
    "scripts": {
        "start": "npm run prod",
        "dev": "echo 'Launching Dev Environment' && npm run build && npm run start-dev-server",
        "build": "echo 'Building:' && webpack --config webpack.config.dev.js --progress --colors",
        "prod": "echo 'Launching Prod Environment' && npm run package && npm run start-server",
        "package": "echo 'Packaging:' && webpack --progress --colors -p --config webpack.config.prod.js",
        "watch": "echo 'Watching:' && webpack --config webpack.config.dev.js --watch --progress --colors",
        "test": "echo 'Testing:' && mocha --compilers js:babel-core/register --recursive --colors",
        "test:watch": "npm test -- --watch ",
        "test:debug": "echo 'Debugging Tests:' && mocha --compilers js:babel-core/register --recursive --colors --debug-brk",
        "start-dev-server": "echo 'Staring Dev Server' && node server.js",
        "start-server": "echo 'Starting Prod Server' && http-server -o -p 3040 dist"
    },
    "author": "Mike Chabot",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mikechabot/react-redux-mui-boilerplate.git"
    },
    "bugs": {
        "url": "https://github.com/mikechabot/react-redux-mui-boilerplate/issues"
    },
    "dependencies": {
        "lodash": "^4.2.0",
        "material-ui": "^0.14.4",
        "moment": "^2.11.1",
        "react": "^0.14.0",
        "react-dom": "^0.14.0",
        "react-redux": "^4.0.6",
        "react-tap-event-plugin": "^0.2.1",
        "redux": "^3.0.5",
        "redux-thunk": "^1.0.3"
    },
    "devDependencies": {
        "autoprefixer": "^6.3.1",
        "babel-core": "^6.0.20",
        "babel-loader": "^6.0.1",
        "babel-polyfill": "^6.3.14",
        "babel-preset-es2015": "^6.0.15",
        "babel-preset-react": "^6.0.15",
        "babel-preset-stage-0": "^6.0.15",
        "css-loader": "^0.23.1",
        "deep-freeze": "0.0.1",
        "expect": "^1.13.4",
        "file-loader": "^0.8.5",
        "html-webpack-plugin": "^1.7.0",
        "mocha": "^2.3.4",
        "node-sass": "^3.4.2",
        "postcss-loader": "^0.8.0",
        "react-hot-loader": "^1.3.0",
        "redux-logger": "^2.6.0",
        "sass-loader": "^3.1.2",
        "sass-resources-loader": "^1.0.1",
        "style-loader": "^0.13.0",
        "svg-react-loader": "^0.3.2",
        "url-loader": "^0.5.7",
        "webpack": "^1.12.2",
        "webpack-dev-server": "^1.12.1"
    }
}
