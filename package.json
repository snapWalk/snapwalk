{
  "name": "react-boilerplate",
  "version": "3.0.2",
  "description": "A slightly opinionated yet dead simple boilerplate for ReactJS",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-prod-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-prod-server": "cross-env NODE_ENV=production node server.js",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "linter": "eslint --ext .jsx --ext .js -o lint-report.html src/"
  },
  "author": "Mike Chabot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mikechabot/react-boilerplate/issues"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "temp-directory": "./.nyc-coverage-temp",
    "report-dir": "./.nyc-coverage"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.7.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "glamor": "^2.20.40",
    "glamorous": "^4.13.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-tabify": "0.1.24",
    "redux": "^4.0.0",
    "redux-entity": "^5.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.11.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "nyc": "^12.0.2",
    "postcss-loader": "^2.1.6",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.3.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.0.3",
    "sinon": "^6.1.3",
    "style-loader": "^0.21.0",
    "svg-react-loader": "^0.4.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
