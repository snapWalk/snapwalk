{
  "name": "react-boilerplate",
  "version": "2.0.0",
  "description": "A slightly opinionated yet dead simple boilerplate for ReactJS",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "test": "cross-env NODE_ENV=test && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha \"test/unit/**/*.js\" ",
    "test:integration": "cross-env NODE_ENV=test mocha \"test/integration/**/*.js\" ",
    "test:coverage": "cross-env NODE_ENV=test && istanbul cover node_modules/mocha/bin/_mocha",
    "test:watch": "npm test -- --watch ",
    "start-dev-server": "cross-env NODE_ENV=development babel-node server.js",
    "start-server": "cross-env NODE_ENV=production babel-node server.js"
  },
  "author": "Mike Chabot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mikechabot/react-boilerplate/issues"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "axios": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "expect": "^1.13.4",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.2.0",
    "mocha": "^2.3.4",
    "moment": "^2.11.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.0",
    "redux-mock-store": "^1.0.2",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "svg-react-loader": "^0.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
